---
import CounterButton from "./CounterButton.astro";
---


<CounterButton 
      data-theme="light"
      aria-label="Custom counter action button"
      text="Custom Action"
      data-count={0}
      id="custom-counter-button"
      title="Click to perform custom action"
      data-step={1}
      disabled={false}
      tabindex="0"
      data-action="custom"
    />